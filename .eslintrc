{
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "operator-linebreak": ["error", "before", { "overrides": { "||": "before" } }],
    "no-underscore-dangle": ["error", { "allow": ["_id"]}],
    "linebreak-style": 0,
    "prettier/prettier": ["error", {
      "endOfLine": "auto"
    }]
  }
}